predictors_test_postop <- test %>% select(REGIONID, CENTERID, PHYSICIANID, SURGYEAR, SURGMONTH, SURGWEEKDAY, AGE, GENDER, BMI, RACE, ETHNICITY, PRIMARYINSURER, RURAL, ADI_NATRANK_MEDIAN, TRANSFER, HTN, DIABETES, PREOP_SMOKING, PRIOR_CAD, PRIOR_CHF, PRIOR_CVD, COPD, PREOP_DIALYSIS, GENHIST, PRIOR_AORSURG, PRIOR_AORSURG_OPENLOC1, PRIOR_AORSURG_OPENLOC2, PRIOR_AORSURG_OPENLOC3, PRIOR_AORSURG_OPENLOC4, PRIOR_AORSURG_ENDOLOC1, PRIOR_AORSURG_ENDOLOC2, PRIOR_AORSURG_ENDOLOC3, PRIOR_AORSURG_ENDOLOC4, PRIOR_CABG, PRIOR_PCI, PRIOR_CEACAS, PRIOR_BYPASS, PRIOR_PVI, PRIOR_MAJAMP,  LIVINGSTATUS, PREOP_FUNCSTATUS, HEMO_L, PREOP_CREAT_UMOL, STRESS, PREOP_EF, PREOP_ASA, PREOP_P2Y, PREOP_STATIN, PREOP_BETABLOCKER, PREOP_ACE, PREOP_ANTICOAG, PATHOLOGY_ANEURYSM_TYPE, PREOP_MAXAAADIA, PROC, DEV_PROXZONE, DEV_DISTZONE, STAGEDAORTRT, TECH, DEV_TYPE, AORDEV_ILIAC, PROX_ZONE_PRIOR_GRAFT, DIST_ZONE_PRIOR_GRAFT, ILIACDEV_INT_R, ILIACDEV_INT_L, BRANCH_INNO_NODISSECT_PRE, BRANCH_LCCA_NODISSECT_PRE, BRANCH_LSUB_NODISSECT_PRE, BRANCH_CELIAC_NODISSECT_PRE, BRANCH_SMA_NODISSECT_PRE, BRANCH_RRENAL_NODISSECT_PRE, BRANCH_LRENAL_NODISSECT_PRE, BRANCH_RCOMILI_NODISSECT_PRE, BRANCH_LCOMILI_NODISSECT_PRE, BRANCH_LSUB_VERTPAT, BRANCH_STAGED, BRANCH_INNO, BRANCH_LCCA, BRANCH_LSUB, BRANCH_CELIAC, BRANCH_SMA, BRANCH_RRENAL, BRANCH_LRENAL, BRANCH_RCOMILI, BRANCH_LCOMILI, BRANCH_INNO_TRT, BRANCH_LCCA_TRT, BRANCH_LSUB_TRT, BRANCH_CELIAC_TRT, BRANCH_SMA_TRT, BRANCH_RRENAL_TRT, BRANCH_LRENAL_TRT, BRANCH_RCOMILI_TRT, BRANCH_LCOMILI_TRT, ANESTHESIA, ABX_START, ABX_STOP, SKINPREP, ACCESS_R, ACCESS_L, ACCESS_R_USGUIDE, ACCESS_L_USGUIDE, ACCESS_R_FEMADJ, ACCESS_L_FEMADJ, ILIACCESS_R, ILIACCESS_L, ILIACCESS_R_PROC, ILIACCESS_L_PROC, LRGST_SHEATH_SIZE_R, LRGST_SHEATH_SIZE_L, ARMNECK_ACCESS, ARMNECK_ACCESS_LOC, AORDEV_NUM, BRANCH_INNO_NUMDEV, BRANCH_LCCA_NUMDEV, BRANCH_LSUB_NUMDEV, BRANCH_CELIAC_NUMDEV, BRANCH_SMA_NUMDEV, BRANCH_RRENAL_NUMDEV, BRANCH_LRENAL_NUMDEV, BRANCH_RCOMILI_NUMDEV, BRANCH_LCOMILI_NUMDEV, IVUSTEE, CL