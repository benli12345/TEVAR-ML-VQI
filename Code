ci.auc(test_nonhispanic$TALE_1yr, pred_nonhispanic)

perf_hispanic <- prediction(pred_hispanic, test_hispanic$TALE_1yr)
perf_hispanic <- performance(perf_hispanic, "tpr", "fpr")

perf_nonhispanic <- prediction(pred_nonhispanic, test_nonhispanic$TALE_1yr)
perf_nonhispanic <- performance(perf_nonhispanic, "tpr", "fpr")

plot(perf_hispanic)
plot(perf_nonhispanic, add = TRUE, col = 'blue')
abline(a=0,b=1)

