#Train and evaluate XGBoost for predicting secondary outcomes

XGB_dialysis_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$dialysis), nrounds = 250, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.2, gamma = 0, colsample_bytree = 0.7, min_child_weight = 1, subsample = 1) #hyperparameters chosen based on above grid search and cross validation
XGB_dialysis_pred_preop <- predict(XGB_dialysis_preop, as.matrix(predictors_test_preop))
auc(test$dialysis, XGB_dialysis_pred_preop)
ci.auc(test$dialysis, XGB_dialysis_pred_preop)
XGB_dialysis_pred_preop_0.5 <- ifelse(XGB_dialysis_pred_preop > 0.5,1,0)
confusionMatrix(as.factor(test$dialysis), as.factor(XGB_dialysis_pred_preop_0.5))

XGB_dialysis_intraop <- xgboost(data = as.matrix(predictors_train_intraop), label = as.numeric(train$dialysis), nrounds = 250, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.2, gamma = 0, colsample_bytree = 0.7, min_child_weight = 1, subsample = 1)
XGB_dialysis_pred_intraop <- predict(XGB_dialysis_intraop, as.matrix(predictors_test_intraop))
auc(test$dialysis, XGB_dialysis_pred_intraop)
ci.auc(test$dialysis, XGB_dialysis_pred_intraop)
XGB_dialysis_pred_intraop_0.5 <- ifelse(XGB_dialysis_pred_intraop > 0.5,1,0)
confusionMatrix(as.factor(test$dialysis), as.factor(XGB_dialysis_pred_intraop_0.5))

XGB_dialysis_postop <- xgboost(data = as.matrix(predictors_train_postop), label = as.numeric(train$dialysis), nrounds = 250, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.2, gamma = 0, colsample_bytree = 0.7, min_child_weight = 1, subsample = 1)
XGB_dialysis_pred_postop <- predict(XGB_dialysis_postop, as.matrix(predictors_test_postop))
auc(test$dialysis, XGB_dialysis_pred_postop)
ci.auc(test$dialysis, XGB_dialysis_pred_postop)
XGB_dialysis_pred_postop_0.5 <- ifelse(XGB_dialysis_pred_postop > 0.5,1,0)
confusionMatrix(as.factor(test$dialysis), as.factor(XGB_dialysis_pred_postop_0.5))

XGB_paralysis_preop <- xgboost(data = as.matrix(predictors_train_preop), label = as.numeric(train$paralysis), nrounds = 250, verbose = TRUE, objective = 'binary:logistic', eval_metric = 'auc', max_depth = 3, eta = 0.2, gamma = 0, colsample_bytree = 0.7, min_child_weight = 1, subsample = 1)
